{
    "name": "ibraheem/dhl-ecommerce-integration",
    "description": "DHL eCommerce Solutions REST API integration for Laravel. Complete package with multi-merchant support, shipments, tracking, rates, labels, webhooks, and admin panel.",
    "type": "library",
    "keywords": [
        "dhl",
        "dhl-ecommerce",
        "shipping",
        "laravel",
        "package",
        "integration",
        "logistics",
        "tracking",
        "webhooks"
    ],
    "license": "MIT",
    "homepage": "https://github.com/IbraheemSalem/dhl-ecommerce-integration",
    "support": {
        "issues": "https://github.com/IbraheemSalem/dhl-ecommerce-integration/issues",
        "source": "https://github.com/IbraheemSalem/dhl-ecommerce-integration"
    },
    "authors": [
        {
            "name": "Ibraheem Salem",
            "email": "sil3nt.code@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "guzzlehttp/guzzle": "^7.8",
        "illuminate/support": "^10.0|^11.0",
        "illuminate/http": "^10.0|^11.0",
        "illuminate/cache": "^10.0|^11.0",
        "illuminate/events": "^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Ibraheem\\DhlEcommerceIntegration\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ibraheem\\DhlEcommerceIntegration\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ibraheem\\DhlEcommerceIntegration\\Providers\\DhlServiceProvider"
            ]
        }
    },
    "require-dev": {
        "orchestra/testbench": "^8.17",
        "phpunit/phpunit": "^10.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
